{
    "name": "telegram-mt-node",
    "version": "0.3.0",
    "description": "Telegram MTProto library (javascript, node.js)",
    "keywords": [
        "telegram",
        "mt",
        "mtproto",
        "mobile",
        "protocol",
        "library"
    ],
    "author": "Enrico Stara <enrico.stara@gmail.com>",
    "homepage": "https://github.com/enricostara/telegram-mt-node",
    "repository": {
        "type": "git",
        "url": "git://github.com/enricostara/telegram-mt-node.git"
    },
    "bugs": {
        "url": "https://github.com/enricostara/telegram-mt-node/issues"
    },
    "license": "MIT",
    "dependencies": {
        "telegram-tl-node": "latest",
        "get-log": "latest",
        "jsbn": "latest",
        "bigint-node": "latest",
        "node-cryptojs-aes": "enricostara/node-cryptojs-aes",
        "http-browserify": "enricostara/http-browserify"
    },
    "devDependencies": {
        "gulp": "latest",
        "gulp-jshint": "latest",
        "gulp-mocha": "latest",
        "should": "latest",
        "mocha": "latest",
        "mocha-better-spec-reporter": "latest",
        "mocha-lcov-reporter": "latest",
        "coveralls": "latest",
        "istanbul": "latest",
        "zuul": "latest"
    },
    "main": "./lib/mtproto",
    "engines": {
        "node": "0.10.x"
    },
    "browser": {
        "http": "http-browserify"
    },
    "scripts": {
        "test": "gulp test",
        "coveralls": "istanbul cover ./node_modules/gulp/bin/gulp.js cover && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
